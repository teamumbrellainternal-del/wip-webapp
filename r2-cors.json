{
  "rules": [
    {
      "allowed": {
        "origins": [
          "https://app.umbrellalive.com",
          "https://umbrellalive.com",
          "https://www.umbrellalive.com",
          "https://umbrella-prod.teamumbrellainternal.workers.dev",
          "https://umbrella-api-staging.teamumbrellainternal.workers.dev",
          "https://umbrella-api-preview.teamumbrellainternal.workers.dev",
          "http://localhost:5173",
          "http://localhost:3000",
          "http://127.0.0.1:5173",
          "http://127.0.0.1:3000"
        ],
        "methods": [
          "GET",
          "PUT",
          "POST",
          "DELETE",
          "HEAD"
        ],
        "headers": [
          "*"
        ]
      },
      "exposed_headers": [
        "ETag",
        "Content-Length",
        "Content-Type"
      ],
      "max_age_seconds": 3600
    }
  ]
}
