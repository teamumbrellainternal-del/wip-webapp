{
  "name": "umbrella-app",
  "version": "0.1.0",
  "description": "Umbrella - Artist marketplace connecting musicians with venues",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "dev:worker": "wrangler dev api/index.ts",
    "build": "tsc && vite build && npm run build:worker",
    "build:worker": "node scripts/build-worker.js",
    "preview": "vite preview",
    "deploy": "npm run build && wrangler deploy",
    "lint": "eslint . --report-unused-disable-directives --max-warnings 30",
    "lint:fix": "eslint . --fix",
    "type-check": "tsc --noEmit",
    "format": "prettier --write \"src/**/*.{ts,tsx}\"",
    "format:check": "prettier --check \"src/**/*.{ts,tsx}\"",
    "test": "vitest run",
    "test:unit": "vitest run tests/unit",
    "test:integration": "vitest run tests/integration",
    "test:e2e": "vitest run tests/e2e",
    "test:watch": "vitest",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest run --coverage",
    "test:smoke": "node scripts/smoke-tests.js",
    "ci": "npm run type-check && npm run lint && npm run test && npm run build",
    "clean": "rm -rf dist node_modules .wrangler",
    "migrate": "wrangler d1 migrations apply umbrella-dev-db --local",
    "migrate:staging": "wrangler d1 migrations apply umbrella-staging-db --env staging",
    "migrate:prod": "wrangler d1 migrations apply umbrella-production-db --env production",
    "migrate:verify": "node scripts/verify-migrations.js",
    "db:create": "wrangler d1 create umbrella-dev-db",
    "kv:create": "wrangler kv:namespace create KV --preview",
    "r2:create": "wrangler r2 bucket create umbrella-dev-media",
    "validate:env": "node scripts/validate-env.js",
    "validate:staging": "node scripts/validate-env.js staging",
    "validate:prod": "node scripts/validate-env.js production",
    "perf:check": "node scripts/performance-check.js",
    "rollback": "node scripts/rollback.js"
  },
  "dependencies": {
    "@radix-ui/react-accordion": "^1.1.2",
    "@radix-ui/react-alert-dialog": "^1.1.15",
    "@radix-ui/react-avatar": "^1.1.10",
    "@radix-ui/react-checkbox": "^1.0.4",
    "@radix-ui/react-dialog": "^1.0.5",
    "@radix-ui/react-dropdown-menu": "^2.0.6",
    "@radix-ui/react-label": "^2.0.2",
    "@radix-ui/react-popover": "^1.0.7",
    "@radix-ui/react-select": "^2.0.0",
    "@radix-ui/react-separator": "^1.0.3",
    "@radix-ui/react-slot": "^1.0.2",
    "@radix-ui/react-switch": "^1.0.3",
    "@radix-ui/react-tabs": "^1.0.4",
    "@radix-ui/react-toast": "^1.1.5",
    "@radix-ui/react-tooltip": "^1.0.7",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.0.0",
    "lucide-react": "^0.263.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-hook-form": "^7.49.2",
    "react-router-dom": "^6.20.0",
    "sonner": "^1.3.1",
    "tailwind-merge": "^1.14.0",
    "tailwindcss-animate": "^1.0.7",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@cloudflare/workers-types": "^4.20231025.0",
    "@commitlint/cli": "^18.4.3",
    "@commitlint/config-conventional": "^18.4.3",
    "@eslint/js": "^9.17.0",
    "@hookform/resolvers": "^3.9.1",
    "@radix-ui/react-aspect-ratio": "^1.1.7",
    "@radix-ui/react-context-menu": "^2.2.16",
    "@radix-ui/react-hover-card": "^1.1.15",
    "@radix-ui/react-menubar": "^1.1.16",
    "@radix-ui/react-navigation-menu": "^1.2.14",
    "@radix-ui/react-progress": "^1.1.7",
    "@radix-ui/react-radio-group": "^1.3.8",
    "@radix-ui/react-scroll-area": "^1.2.10",
    "@radix-ui/react-slider": "^1.3.6",
    "@radix-ui/react-toggle": "^1.1.10",
    "@radix-ui/react-toggle-group": "^1.1.11",
    "@types/react": "^18.2.15",
    "@types/react-dom": "^18.2.7",
    "@typescript-eslint/eslint-plugin": "^8.18.2",
    "@typescript-eslint/parser": "^8.18.2",
    "@vitejs/plugin-react": "^4.3.4",
    "@vitest/ui": "^4.0.3",
    "autoprefixer": "^10.4.14",
    "cmdk": "^1.1.1",
    "embla-carousel-react": "^8.6.0",
    "eslint": "^9.17.0",
    "eslint-plugin-react-hooks": "^5.1.0",
    "eslint-plugin-react-refresh": "^0.4.16",
    "globals": "^16.4.0",
    "input-otp": "^1.4.2",
    "postcss": "^8.4.27",
    "prettier": "^3.1.0",
    "prettier-plugin-tailwindcss": "^0.5.7",
    "react-day-picker": "^9.11.1",
    "react-resizable-panels": "^3.0.6",
    "recharts": "^3.3.0",
    "tailwindcss": "^3.3.3",
    "typescript": "^5.0.2",
    "typescript-eslint": "^8.18.2",
    "vaul": "^1.1.2",
    "vite": "^6.1.0",
    "vitest": "^4.0.3",
    "wrangler": "^4.33.1"
  },
  "engines": {
    "node": ">=20.0.0",
    "npm": ">=10.0.0"
  }
}
